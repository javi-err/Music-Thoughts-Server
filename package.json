{
  "name": "express-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require test/setup.js --timeout 10000",
    "dev": "nodemon src/server.js",
    "migrate": "postgrator --config postgrator-config.js",
    "migrate:test": "env NODE_ENV=test npm run migrate",
    "migrate:production": "set env SSL=true DATABASE_URL=$(heroku config:get DATABASE_URL) && npm run migrate",
    "start": "node src/server.js",
    "predeploy": "npm audit",
    "deploy": "git push heroku master"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chai": "^4.2.0",
    "cipm": "^2.0.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "jsonparser": "^1.0.1",
    "knex": "^0.21.0",
    "morgan": "^1.10.0",
    "pg": "^8.2.1",
    "postgrator": "^4.0.0",
    "postgrator-cli": "^3.3.0",
    "supertest": "^4.0.2",
    "xss": "^1.0.6"
  },
  "devDependencies": {}
}
